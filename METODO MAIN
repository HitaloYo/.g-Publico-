// Classe Conta
public class Conta {
    private String titular;
    private int numero;
    private String agencia;
    private double saldo;

    // Métodos saca, deposita, calculaRendimento e getDadosConta
    public void saca(double valor) {
        if (valor > 0 && valor <= saldo) {
            saldo -= valor;
            System.out.println("Saque de R$ " + valor + " realizado com sucesso.");
        } else {
            System.out.println("Saldo insuficiente para saque.");
        }
    }

    public void deposita(double valor) {
        if (valor > 0) {
            saldo += valor;
            System.out.println("Depósito de R$ " + valor + " realizado com sucesso.");
        } else {
            System.out.println("Valor de depósito inválido.");
        }
    }

    public double calculaRendimento() {
        return saldo * 0.1; // Calcula rendimento como 10% do saldo
    }

    public String getDadosConta() {
        return "Titular: " + titular + "\nNúmero da conta: " + numero + "\nAgência: " + agencia + "\nSaldo: R$ " + saldo;
    }

    // Getters e setters
    public String getTitular() {
        return titular;
    }

    public void setTitular(String titular) {
        this.titular = titular;
    }

    public int getNumero() {
        return numero;
    }

    public void setNumero(int numero) {
        this.numero = numero;
    }

    public String getAgencia() {
        return agencia;
    }

    public void setAgencia(String agencia) {
        this.agencia = agencia;
    }

    public double getSaldo() {
        return saldo;
    }

    public void setSaldo(double saldo) {
        this.saldo = saldo;
    }
}
